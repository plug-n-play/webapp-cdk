{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["node_modules/@types"],
    "skipLibCheck": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "lib": ["es6"],
    "target": "es6"
  }
}
